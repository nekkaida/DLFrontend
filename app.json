{
  "expo": {
    "name": "dl-frontend",
    "slug": "deuce-league",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dl-logo.png",
    "scheme": "deuceleague",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show nearby leagues and courts.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to show nearby leagues and courts.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.deucelague.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dl-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.deuceleague.app",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "usesCleartextTraffic": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/dl-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/dl-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Deuce League to use your location to find nearby courts and leagues.",
          "locationAlwaysPermission": "Allow Deuce League to use your location to find nearby courts and leagues.",
          "locationWhenInUsePermission": "Allow Deuce League to use your location to find nearby courts and leagues."
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e50b53ee-0b2f-4246-8e17-18865e579166"
      }
    },
    "owner": "nexea"
  }
}
